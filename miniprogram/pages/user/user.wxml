<import src="../template/tabBar.wxml" />
<template is="tabBar" data="{{tabBar: tabBar}}" /> 
<view class="bg_img">
  <view class="user-img"><open-data type="userAvatarUrl" class="user-img-url"></open-data></view>
</view>
<view class="user-txt"><open-data type="userNickName" lang="zh_CN" class="user-txt-name"></open-data></view>
<view class="user-canvas">
  <canvas canvas-id="myCanvas" class="canvas"></canvas>
</view>